#source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '12.2'
use_frameworks!
project 'AR Locations.xcodeproj'

def tourist_pods
   pod 'Alamofire'
   pod 'AlamofireImage'
   pod 'SwiftyJSON'
   pod 'ARCoreLocation'
end


target 'AR Locations' do
	tourist_pods
end

target 'AR LocationsTests' do
	tourist_pods
end
